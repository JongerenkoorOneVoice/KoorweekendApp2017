<?xml version="1.0" encoding="UTF-8"?>
<ContentPage
		xmlns="http://xamarin.com/schemas/2014/forms"
		xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
		x:Class="KoorweekendApp2017.Pages.ContactSinglePage"
		Title="{Binding FirstName}">
	<ContentPage.Content>
		<StackLayout VerticalOptions="FillAndExpand">
		<Label Text="{Binding FirstName}"></Label>
		<Label Text="{Binding LastName}"></Label>
			<Label Text="{Binding Address}"></Label>
			<Label Text="{Binding AreaCode}"></Label>
			<Label Text="{Binding City}"></Label>
			<Label Text="{Binding Phone1}"></Label>
			<Label Text="{Binding Mobile1}"></Label>
			<Label Text="{Binding Email1}"></Label>
			<Label Text="{Binding BirthDate}"></Label>
			<Label Text="{Binding StartDate}"></Label>
	
			

			</StackLayout>
	</ContentPage.Content>
</ContentPage>
