<?xml version="1.0" encoding="UTF-8"?>
<ContentPage
		xmlns="http://xamarin.com/schemas/2014/forms"
		xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
		x:Class="KoorweekendApp2017.Pages.EventSinglePage"
		Title="{Binding Title}"
		Padding="15,15,15,0">
	<ContentPage.Content>
		<StackLayout VerticalOptions="FillAndExpand">
			<Label Text="{Binding Title}" HorizontalTextAlignment="Center"></Label>
			<Label Text="{Binding StartDate}" HorizontalTextAlignment="Center"></Label>
			<Label Text="{Binding StartTime}" HorizontalTextAlignment="Center"></Label>
			<Label Text="{Binding EndDate}" HorizontalTextAlignment="Center"></Label>
			<Label Text="{Binding EndTime}" HorizontalTextAlignment="Center"></Label>
			<Label Text="{Binding Address}" HorizontalTextAlignment="Center"></Label>
			<Label Text="{Binding AreaCode}" HorizontalTextAlignment="Center"></Label>
			<Label Text="{Binding City}" HorizontalTextAlignment="Center"></Label>
		</StackLayout>
	</ContentPage.Content>
</ContentPage>
